{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "shadcn-registry",
  "main": "src/index.ts",
  "compatibility_date": "2025-06-03",
  "kv_namespaces": [
    {
      "binding": "KV",
      "id": "your-kv-namespace-id",
    },
  ],
  "assets": {
    "directory": "public",
    "binding": "REGISTRY",
    "run_worker_first": true,
  },
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1,
  },
}
